#include <stdio.h>
#include <queue>
using namespace std;
int n;
priority_queue<int, vector<int>, greater<int>> pq;
int main(){
  scanf("%d", &n);
  int val;
  for(int i=0;i<n;i++) {
    scanf("%d", &val);
    if(val == 0) {
      if(pq.size() > 0){
        printf("%d\n", pq.top());
        pq.pop();
      }
      else printf("0\n");
    }
    else pq.push(val);
  }
  return 0;
}
