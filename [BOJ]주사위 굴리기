#include <stdio.h>
const int MAX = 25;

int n, m, x, y, k;
int dice[4][3], arr[MAX][MAX];
int dx[] = { 0, 0, 0, -1, 1 };
int dy[] = { 0, 1, -1, 0, 0 };

int main() {
	scanf("%d %d %d %d %d", &n, &m, &x, &y, &k);

	for (int i = 0; i < n; i++) {
		for (int j = 0; j < m; j++) {
			scanf("%d", &arr[i][j]);
		}
	}

	for (int i = 0; i < k; i++) {
		int dir;
		scanf("%d", &dir);

		int nx = x + dx[dir];
		int ny = y + dy[dir];
		
		if (nx < 0 || nx >= n || ny < 0 || ny >= m) continue;
		if (dir == 1) {
			int tmp = dice[1][1];
			dice[1][1] = dice[1][0], dice[1][0] = dice[3][1], dice[3][1] = dice[1][2], dice[1][2] = tmp;
		}
		else if (dir == 2) {
			int tmp = dice[1][1];
			dice[1][1] = dice[1][2], dice[1][2] = dice[3][1], dice[3][1] = dice[1][0], dice[1][0] = tmp;
		}
		else if (dir == 3) {
			int tmp = dice[0][1];
			dice[0][1] = dice[1][1], dice[1][1] = dice[2][1], dice[2][1] = dice[3][1], dice[3][1] = tmp;
		}
		else {
			int tmp = dice[0][1];
			dice[0][1] = dice[3][1], dice[3][1] = dice[2][1], dice[2][1] = dice[1][1], dice[1][1] = tmp;
		}

		if (arr[nx][ny] == 0) arr[nx][ny] = dice[3][1];
		else {
			dice[3][1] = arr[nx][ny];
			arr[nx][ny] = 0;
		}
		x = nx, y = ny;
		printf("%d\n", dice[1][1]);
	}
	return 0;
}
